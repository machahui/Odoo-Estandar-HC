<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_nogiveback_mail_notification" model="ir.cron">
        <field name="name">Enviar herramientas no devueltas por mail</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_res_config_settings"/>
        <field name="state">code</field>
        <field name="code">model.action_nogiveback_tools_send()</field>
        <field name="active" eval="True" />
    </record>
</odoo>
